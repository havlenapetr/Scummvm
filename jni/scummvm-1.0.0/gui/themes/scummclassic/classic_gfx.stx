					bevel = '2'
					fill = 'none'
		/>
		<drawstep	func = 'cross'
					fill = 'foreground'
					stroke = '2'
					fg_color = 'green'
		/>
	</drawdata>

	<drawdata id = 'checkbox_default' cache = 'false'>
		<text	font = 'text_default'
